--
-- Table structure for table `cmf_stock`
--

DROP TABLE IF EXISTS `cmf_stock`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `cmf_stock` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(100) NOT NULL DEFAULT '' COMMENT '股票名称',
  `code` char(6) NOT NULL DEFAULT '0' COMMENT '股票代码',
  `code0` char(8) NOT NULL DEFAULT '' COMMENT '查询股票代码，如sh600001',
  `status` tinyint(4) NOT NULL DEFAULT '1' COMMENT '1=>''正常'', 2=>''停盘'',3=>''不在业务范围'', 4=>''已废弃'', ',
  `dsc` varchar(200) NOT NULL DEFAULT '' COMMENT '描述',
  `time` int(11) NOT NULL DEFAULT '0' COMMENT '更新时间',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=4710 DEFAULT CHARSET=utf8mb4 COMMENT='股票列表';
/*!40101 SET character_set_client = @saved_cs_client */;

