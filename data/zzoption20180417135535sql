-- Table structure for table `cmf_collect`
--

DROP TABLE IF EXISTS `cmf_collect`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `cmf_collect` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT NULL COMMENT '用户',
  `stock` int(11) DEFAULT NULL COMMENT '股票id',
  PRIMARY KEY (`id`),
  KEY `uid` (`uid`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8mb4 COMMENT='用户自选股票';
/*!40101 SET character_set_client = @saved_cs_client */;

