DROP TABLE IF EXISTS `cmf_stock_news`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `cmf_stock_news` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '新闻ID',
  `cate_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT '分类ID',
  `type` tinyint(4) unsigned NOT NULL DEFAULT '0' COMMENT '来源类型：与source对应',
  `source` varchar(50) NOT NULL DEFAULT '本站' COMMENT '来源：本地、证券时报网、新浪财经',
  `link` tinytext COMMENT '外链',
  `title` varchar(255) NOT NULL DEFAULT '' COMMENT '新闻标题',
  `content` text COMMENT '新闻内容',
  `create_time` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '创建时间',
  `list_order` float unsigned NOT NULL DEFAULT '10000' COMMENT '排序：从小到大',
  `shop` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '0系统 1主站 N代理',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=320 DEFAULT CHARSET=utf8mb4;
/*!40101 SET character_set_client = @saved_cs_client */;

