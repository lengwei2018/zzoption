/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `cmf_stock_indice` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(100) NOT NULL DEFAULT '' COMMENT '股票名称',
  `code` char(10) NOT NULL DEFAULT '0' COMMENT '股票代码',
  `count` decimal(8,2) NOT NULL DEFAULT '0.00' COMMENT '总点数',
  `num` char(10) NOT NULL DEFAULT '0.00' COMMENT '涨幅点数',
  `percent` char(5) NOT NULL DEFAULT '0.00' COMMENT '涨幅%',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COMMENT='股票列表';
/*!40101 SET character_set_client = @saved_cs_client */;

