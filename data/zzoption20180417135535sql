  `name` varchar(50) NOT NULL DEFAULT '' COMMENT '股票名称',
  `uid` int(11) NOT NULL DEFAULT '0' COMMENT '用户',
  `uname` varchar(50) NOT NULL DEFAULT '' COMMENT '用户名称',
  `oid` char(20) NOT NULL DEFAULT '' COMMENT '订单号',
  `money0` int(12) NOT NULL DEFAULT '0' COMMENT '名义本金',
  `money1` decimal(12,2) NOT NULL DEFAULT '0.00' COMMENT '权利金',
  `month` tinyint(4) NOT NULL DEFAULT '1' COMMENT '行权周期（月）',
  `status` tinyint(4) NOT NULL DEFAULT '0' COMMENT '0询价，1询价有结果，2询价失败，3买入，4买入成功，5买入失败，6行权中，7卖出，8过期',
  `time` int(11) NOT NULL DEFAULT '0' COMMENT '最新时间',
  `inquiry_time` int(11) NOT NULL DEFAULT '0' COMMENT '询价时间',
  `buy_time` int(11) NOT NULL DEFAULT '0' COMMENT '购买时间',
  `sell_time` int(11) NOT NULL DEFAULT '0' COMMENT '平仓时间',
  `end_time` int(11) NOT NULL DEFAULT '0' COMMENT '行权期限',
  `notice_time` int(11) NOT NULL DEFAULT '0' COMMENT '短信提示时间',
  `price1` decimal(10,2) NOT NULL DEFAULT '0.00' COMMENT '期初价格',
  `price2` decimal(10,2) NOT NULL DEFAULT '0.00' COMMENT '期末价格',
  `price1_0` decimal(10,4) NOT NULL DEFAULT '0.0000' COMMENT '期初价格，原始',
  `price2_0` decimal(10,4) NOT NULL DEFAULT '0.0000' COMMENT '期末价格，原始',
  `money2` decimal(12,2) NOT NULL DEFAULT '0.00' COMMENT '实际盈利，卖出价格',
  `dsc` varchar(255) NOT NULL DEFAULT '' COMMENT '订单备注',
  `is_old` tinyint(4) NOT NULL DEFAULT '0' COMMENT '是否过期，0正常，1过期,2可以行权，3即将过期',
  PRIMARY KEY (`id`),
  UNIQUE KEY `oid` (`oid`) COMMENT '订单号唯一',
  KEY `status` (`status`) COMMENT '状态',
  KEY `uid` (`uid`) COMMENT '用户',
  KEY `is_old` (`is_old`) COMMENT '订单的时间状态，是否过期和可以行权'
) ENGINE=MyISAM AUTO_INCREMENT=11 DEFAULT CHARSET=utf8mb4 COMMENT='用户询价买入订单表';
/*!40101 SET character_set_client = @saved_cs_client */;

